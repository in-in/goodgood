- var pager = true

extends inc/temp

append var
    - var thisPage = 'Portfolio'
    - var pageTitle = pageTitle.concat(' | ' + thisPage)

block index

    block hero
        include inc/hero

    block main-portfolio
        include inc/main-portfolio

    block testimonials
        include inc/testimonials
    if pager
        append script
            script(src="js/pager.js")